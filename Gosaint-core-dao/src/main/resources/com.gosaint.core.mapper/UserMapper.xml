<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!--namespace:命名空间,映射类的唯一标识符-->
<mapper namespace="com.gosaint.core.mapper.UserMapper">
    <!--resultType:返回类型：Employee
        id 查询的唯一标识-->
    <select id="findEmployeeById" resultType="user">
        SELECT  * FROM  t_user WHERE id = #{id}
    </select>
</mapper>